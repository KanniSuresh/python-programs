   "source": [
    "a=list(map(int,input(\"enter the list: \").split()))\n",
    "evn=[]\n",
    "odd=[]\n",
    "\n",
    "\n",
    "for i in a:\n",
    "    if i%2==0:\n",
    "        evn=evn+[i]\n",
    "    else:\n",
    "        odd=odd+[i]\n",
    "evn.sort()\n",
    "odd.sort()\n",
    "print(\"Even:\",evn)\n",
    "print(\"Odd:\",odd)\n",
    "print(\"Largest in even: \",evn[-1])\n",
    "print(\"Largest in odd: \",odd[-1])"
   ]
  }
